{
  "name": "sirius",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-electron": "ng build --base-href . && copy /Y src\\electron\\* dist\\",
    "electron": "npm run build-electron && .\\node_modules\\.bin\\electron dist\\",
    "execute-electron": ".\\node_modules\\.bin\\electron dist\\",
    "electron-package": "ng build --prod && electron-packager . --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.3",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.3",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@electron/remote": "github:electron/remote",
    "@types/electron": "^1.6.10",
    "angularx-social-login": "^4.0.1",
    "bootstrap": "^5.1.3",
    "fs": "^0.0.1-security",
    "ngx-electron": "^2.2.0",
    "node": "^17.4.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.42",
    "electron": "^16.0.7",
    "electron-package": "^0.1.0",
    "file-system": "^2.2.2",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}
